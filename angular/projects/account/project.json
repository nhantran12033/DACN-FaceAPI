{
  "name": "account",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/account/src",
  "prefix": "abp",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/packages/account",
        "{workspaceRoot}/dist/packages/account/admin",
        "{workspaceRoot}/dist/packages/account/admin/config",
        "{workspaceRoot}/dist/packages/account/public",
        "{workspaceRoot}/dist/packages/account/public/config"
      ],
      "options": {
        "tsConfig": "packages/account/tsconfig.lib.json",
        "project": "packages/account/ng-package.json",
        "updateBuildableProjectDepsInPackageJson": true
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/account/tsconfig.lib.prod.json"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["packages/account/src/**/*.ts", "packages/account/src/**/*.html"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/account"],
      "options": {
        "jestConfig": "packages/account/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["commercial-ui"]
}
